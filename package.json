{
  "name": "hurryposdash",
  "version": "19.0.3",
  "private": true,
  "main": "main.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "desktop:dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "desktop:dev:no-optional": "concurrently -k \"npm run dev\" \"wait-on http://localhost:5173 && cross-env npm_config_optional=false VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "desktop:build": "cross-env VITE_TARGET=electron VITE_API_URL=https://hurrypos-backend.onrender.com/api vite build && electron-builder --publish=never",
    "desktop:dist": "cross-env VITE_TARGET=electron VITE_API_URL=https://hurrypos-backend.onrender.com/api vite build && electron .",
    "desktop:dev:render": "concurrently -k \"cross-env VITE_API_URL=https://hurrypos-backend.onrender.com/api vite\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "postinstall": "node -e \"if(process.env.BEYPRO_DESKTOP==='1'){require('child_process').execSync('npx electron-builder install-app-deps',{stdio:'inherit'})}\""
  },
  "build": {
    "appId": "com.beypro.pos",
    "productName": "Beypro POS",
    "directories": {
      "buildResources": "build",
      "output": "out"
    },
    "files": [
      "dist/**",
      "main.js",
      "preload.js",
      "package.json",
      "node_modules/**"
    ],
    "asarUnpack": [
      "node_modules/@thesusheer/electron-printer/**",
      "**/*.node"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.business"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowElevation": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@mapbox/polyline": "^1.2.1",
    "@mui/material": "^7.0.1",
    "@mui/x-data-grid": "^7.28.3",
    "@mui/x-date-pickers": "^7.28.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "chart.js": "^4.4.8",
    "date-fns": "^2.29.3",
    "file-saver": "^2.0.5",
    "html5-qrcode": "^2.3.8",
    "i18next": "^25.1.1",
    "jsqr": "^1.4.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.479.0",
    "nodemailer": "^6.10.0",
    "pdf-to-printer": "^5.6.1",
    "polyline": "^0.2.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-clock": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.1",
    "react-leaflet": "^4.2.1",
    "react-modal": "^3.16.3",
    "react-qr-code": "^2.0.17",
    "react-qrcode-logo": "^4.0.0",
    "react-rnd": "^10.5.2",
    "react-router-dom": "^7.6.0",
    "react-swipeable": "^7.0.2",
    "react-time-picker": "^7.0.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.28.1",
    "react-webcam": "^7.2.0",
    "recharts": "^2.15.1",
    "socket.io-client": "^4.8.1",
    "streamifier": "^0.1.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-scrollbar-hide": "^1.3.1",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@vitejs/plugin-react": "^4.4.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "^18.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "http-proxy-middleware": "^3.0.5",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "source-map-loader": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0",
    "wait-on": "^8.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
